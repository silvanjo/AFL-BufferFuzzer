    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug AFL (bug)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/afl-fuzz",
            "args": ["-m", "none", "-i", "targets/bug_test/input_dir", "-o", "targets/bug_test/output_dir", "--", "./targets/bug_test/bug", "@@"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
